{
  "name": "sunny-golovine-dot-com",
  "private": true,
  "description": "My Personal Website",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "start:netlify": "netlify dev",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "clean": "gatsby clean && node scripts/clean-project.js && yarn install",
    "typecheck": "tsc",
    "ci": "yarn format && yarn typecheck",
    "update:check": "ncu",
    "update:run": "ncu -u",
    "resume:start": "vite resume",
    "resume:build": "vite build resume --outDir ../resume-dist",
    "resume:serve": "serve -s resume-dist",
    "resume:export": "node ./scripts/generate-resume.js",
    "generate-resume": "npm-run-all resume:build --parallel resume:serve resume:export",
    "cms:generate:dev": "NODE_ENV=development node ./scripts/generate-cms-config.js",
    "cms:generate:prod": "NODE_ENV=production node ./scripts/generate-cms-config.js",
    "cms:lint": "yamllint ./static/admin/config.yml",
    "prestart": "yarn cms:generate:dev",
    "prebuild": "yarn cms:generate:prod",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@datapunt/matomo-tracker-react": "^0.4.0",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@sendgrid/mail": "^7.4.4",
    "@tailwindcss/typography": "^0.4.1",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "dompurify": "^2.2.9",
    "gatsby": "^3.6.2",
    "gatsby-plugin-mdx": "^2.6.0",
    "gatsby-plugin-netlify-cms": "^5.7.1",
    "gatsby-plugin-plausible": "^0.0.7",
    "gatsby-plugin-postcss": "^4.6.0",
    "gatsby-plugin-preact": "^5.6.0",
    "gatsby-plugin-react-helmet": "^4.6.0",
    "gatsby-plugin-sharp": "^3.6.0",
    "gatsby-plugin-typescript": "^3.6.0",
    "gatsby-remark-images": "^5.3.0",
    "gatsby-source-filesystem": "^3.6.0",
    "gatsby-transformer-json": "^3.6.0",
    "jsdom": "^16.6.0",
    "netlify-cms-app": "^2.15.19",
    "preact": "^10.5.13",
    "preact-render-to-string": "^5.1.19",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "unzalgo": "^2.1.2",
    "use-media": "^1.4.0"
  },
  "devDependencies": {
    "@alexlafroscia/yaml-merge": "^4.0.0",
    "@babel/core": "^7.14.3",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/classnames": "^2.3.1",
    "@types/react-dom": "^17.0.7",
    "@types/react-helmet": "^6.1.1",
    "autoprefixer": "^10.2.6",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-gatsby": "^1.6.0",
    "depcheck": "^1.4.1",
    "netlify-cli": "^3.35.0",
    "npm-check-updates": "^11.6.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.4.7",
    "postcss": "^8.3.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "2.3.1",
    "puppeteer": "^10.0.0",
    "serve": "^12.0.0",
    "tailwindcss": "^2.1.4",
    "typescript": "^4.3.2",
    "yaml-lint": "^1.2.4"
  },
  "engines": {
    "node": ">=14"
  }
}
