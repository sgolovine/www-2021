{
  "name": "sunny-golovine-dot-com",
  "private": true,
  "description": "My Personal Website",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "run-p start-cms-proxy start",
    "build": "gatsby build",
    "dry-build": "NODE_ENV=development yarn fetch-dry-build-files && gatsby build",
    "start": "gatsby develop",
    "start-cold": "NODE_ENV=development yarn fetch-dry-build-files && gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "clean": "gatsby clean && node scripts/clean-project.js && yarn install",
    "typecheck": "tsc",
    "lint": "eslint 'src/**/*.{ts,tsx}' 'scripts/**/*.js'",
    "ci": "run-s format typecheck lint find-unused-code find-unused-packages",
    "find-unused-code": "ts-prune -p ./tsconfig.json --error",
    "find-unused-packages": "depcheck --config=./.depcheckrc.yml",
    "serve": "gatsby serve",
    "cms:generate:dev": "NODE_ENV=development node ./scripts/generate-cms-config.js",
    "cms:generate:prod": "NODE_ENV=production node ./scripts/generate-cms-config.js",
    "cms:lint": "yamllint ./static/admin/_config/*.yml",
    "fetch-remote-posts": "node ./scripts/fetch-remote-blog-posts.js",
    "fetch-dry-build-files": "node ./scripts/generate-dry-build-files.js",
    "start-cms-proxy": "netlify-cms-proxy-server",
    "prestart": "yarn cms:generate:dev && yarn fetch-remote-posts",
    "prebuild": "yarn cms:generate:prod && yarn fetch-remote-posts",
    "resume:dev": "node ./scripts/resume/devServer.js",
    "resume:build": "node ./scripts/resume/buildServer.js",
    "vite:server:dev": "ts-node ./internals/server.dev.ts",
    "vite:server:prod": "ts-node ./internals/server.prod.ts",
    "vite:dev": "yarn vite:server:dev",
    "vite:build": "vite build && vite build --ssr && vite-plugin-ssr prerender"
  },
  "dependencies": {
    "@mdx-js/mdx": "v1",
    "@mdx-js/react": "v1",
    "@sendgrid/mail": "^7.7.0",
    "@tailwindcss/typography": "^0.5.2",
    "@vitejs/plugin-react": "^1.3.2",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "copy-to-clipboard": "^3.3.1",
    "dayjs": "^1.11.3",
    "gatsby": "^4.16.0",
    "gatsby-plugin-image": "^2.16.1",
    "gatsby-plugin-mdx": "^3.16.1",
    "gatsby-plugin-netlify-cms": "^6.16.0",
    "gatsby-plugin-postcss": "^5.16.0",
    "gatsby-plugin-preact": "^6.16.0",
    "gatsby-plugin-react-helmet": "^5.16.0",
    "gatsby-plugin-sharp": "^4.16.1",
    "gatsby-plugin-sitemap": "^5.16.0",
    "gatsby-plugin-typescript": "^4.16.0",
    "gatsby-remark-images": "^6.16.0",
    "gatsby-source-filesystem": "^4.16.0",
    "gatsby-transformer-json": "^4.16.0",
    "gatsby-transformer-sharp": "^4.16.0",
    "glob": "^8.0.3",
    "gutenberg-css": "^0.6.1",
    "handlebars": "^4.7.7",
    "netlify-cms-app": "^2.15.72",
    "node-http-server": "^8.1.5",
    "preact": "^10.8.1",
    "preact-render-to-string": "^5.2.0",
    "prism-react-renderer": "^1.3.3",
    "puppeteer": "^14.4.1",
    "react": "^18",
    "react-dom": "^18",
    "react-headroom": "^3.2.0",
    "react-helmet": "^6.1.0",
    "uuid": "^8.3.2",
    "vite": "^2.9.12",
    "vite-plugin-ssr": "^0.3.64"
  },
  "devDependencies": {
    "@alexlafroscia/yaml-merge": "^4.1.0",
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@types/classnames": "^2.3.1",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "@types/mdx-js__react": "^1.5.5",
    "@types/node": "^18.0.0",
    "@types/react-dom": "^18.0.5",
    "@types/react-headroom": "^2.2.2",
    "@types/react-helmet": "^6.1.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "autoprefixer": "^10.4.7",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-gatsby": "^2.16.0",
    "compression": "^1.7.4",
    "cssnano": "^5.1.11",
    "depcheck": "^1.4.3",
    "dotenv": "^16.0.1",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.18.1",
    "http-server": "^14.1.1",
    "netlify-cms-proxy-server": "^1.3.24",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss": "^8.4.14",
    "prettier": "2.7.1",
    "serve": "^13.0.2",
    "tailwindcss": "^3.1.3",
    "ts-node": "^10.8.1",
    "ts-prune": "^0.10.3",
    "typescript": "^4.7.3",
    "yaml-lint": "^1.7.0"
  },
  "engines": {
    "node": ">=16.13.x",
    "npm": "USE_YARN"
  }
}
