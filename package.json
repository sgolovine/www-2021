{
  "name": "sunny-golovine-dot-com",
  "private": true,
  "description": "My Personal Website",
  "version": "2.1.0",
  "license": "MIT",
  "scripts": {
    "dev": "yarn build-post-indexes && yarn generate-version && run-p cms:proxy start",
    "prebuild": "yarn build-post-indexes && yarn generate-version",
    "build": "next build && next export -o dist && next-image-export-optimizer",
    "start": "next dev --port 8080",
    "serve": "serve -s dist",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "tsc": "tsc",
    "tsc -w": "tsc --watch --noEmit",
    "lint": "next lint",
    "ci": "run-s format tsc lint find-unused-code find-unused-packages",
    "find-unused-code": "ts-prune -p ./tsconfig.json --error",
    "find-unused-packages": "depcheck --config=./.depcheckrc.yml",
    "cms:proxy": "netlify-cms-proxy-server",
    "resume:dev": "node ./scripts/resume/devServer.js",
    "resume:build": "node ./scripts/resume/buildServer.js",
    "build-post-indexes": "node ./scripts/create-post-index.js",
    "generate-version": "node ./scripts/generate-version.js"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "@tailwindcss/typography": "^0.5.2",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.3",
    "glob": "^8.0.3",
    "gray-matter": "^4.0.3",
    "handlebars": "^4.7.7",
    "netlify-cms-app": "^2.15.72",
    "next": "^12.2.0",
    "next-image-export-optimizer": "^0.11.0",
    "next-mdx-remote": "^4.0.3",
    "next-seo": "^5.4.0",
    "node-http-server": "^8.1.5",
    "numeral": "^2.0.6",
    "puppeteer": "^14.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-headroom": "^3.2.0",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@mapbox/rehype-prism": "^0.8.0",
    "@types/glob": "^7.2.0",
    "@types/numeral": "^2.0.2",
    "@types/react": "^18.0.15",
    "@types/react-headroom": "^2.2.2",
    "@types/styled-components": "^5.1.25",
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.11",
    "depcheck": "^1.4.3",
    "dotenv": "^16.0.1",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "netlify-cms-core": "^2.55.2",
    "netlify-cms-proxy-server": "^1.3.24",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "2.7.1",
    "serve": "^13.0.4",
    "tailwindcss": "^3.1.3",
    "ts-prune": "^0.10.3",
    "typescript": "^4.7.3"
  },
  "resolutions": {
    "styled-components": "^5",
    "react": "^18",
    "react-dom": "^18"
  },
  "engines": {
    "node": ">=16.13.x"
  }
}
