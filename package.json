{
  "name": "sunny-golovine-dot-com",
  "private": true,
  "description": "My Personal Website",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "predev": "run-s cms:generate:dev map-post-slugs fetch-remote-posts",
    "prebuild": "run-s cms:generate:prod map-post-slugs fetch-remote-posts",
    "dev": "run-p cms:start-proxy ssr:server:dev",
    "build": "yarn cms:generate:dev && vite build && vite build --ssr && vite-plugin-ssr prerender",
    "serve": "serve -s dist/client",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"scripts/**/*.js\"",
    "clean": "node scripts/clean-project.js && yarn install",
    "tsc": "tsc",
    "tsc -w": "tsc --watch",
    "dead-code": "ts-prune -p ./tsconfig.json --error",
    "depcheck": "depcheck --config=./.depcheckrc.yml",
    "ci": "run-s format tsc lint dead-code depcheck",
    "cms:generate:dev": "NODE_ENV=development node ./scripts/generate-cms-config.js",
    "cms:generate:prod": "NODE_ENV=production node ./scripts/generate-cms-config.js",
    "cms:lint": "yamllint ./static/admin/_config/*.yml",
    "cms:start-proxy": "netlify-cms-proxy-server",
    "resume:dev": "node ./scripts/resume/devServer.js",
    "resume:build": "node ./scripts/resume/buildServer.js",
    "ssr:server:dev": "ts-node ./internals/server.dev.ts",
    "ssr:server:prod": "ts-node ./internals/server.prod.ts",
    "map-post-slugs": "node ./scripts/slug-mapper.js",
    "fetch-remote-posts": "node ./scripts/fetch-remote-posts.js"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "@tailwindcss/typography": "^0.5.2",
    "@vitejs/plugin-react": "^1.3.2",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "copy-to-clipboard": "^3.3.1",
    "dayjs": "^1.11.3",
    "glob": "^8.0.3",
    "gray-matter": "^4.0.3",
    "gutenberg-css": "^0.6.1",
    "handlebars": "^4.7.7",
    "html-minifier": "^4.0.0",
    "netlify-cms-app": "^2.15.72",
    "node-http-server": "^8.1.5",
    "prism-react-renderer": "^1.3.3",
    "prismjs": "^1.28.0",
    "puppeteer": "^14.4.1",
    "react": "^18",
    "react-dom": "^18",
    "react-headroom": "^3.2.0",
    "react-helmet": "^6.1.0",
    "rehype-format": "^4.0.1",
    "rehype-stringify": "^9.0.3",
    "remark": "^14.0.2",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-prism": "^1.3.6",
    "remark-rehype": "^10.1.0",
    "unified": "^10.1.2",
    "uuid": "^8.3.2",
    "vite": "^2.9.12",
    "vite-plugin-ssr": "^0.3.64"
  },
  "devDependencies": {
    "@alexlafroscia/yaml-merge": "^4.1.0",
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@types/classnames": "^2.3.1",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "@types/node": "^18.0.0",
    "@types/prismjs": "^1.26.0",
    "@types/react-dom": "^18.0.5",
    "@types/react-headroom": "^2.2.2",
    "@types/react-helmet": "^6.1.5",
    "@types/remark-prism": "^1.3.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "autoprefixer": "^10.4.7",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-gatsby": "^2.16.0",
    "compression": "^1.7.4",
    "cssnano": "^5.1.11",
    "depcheck": "^1.4.3",
    "dotenv": "^16.0.1",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.18.1",
    "netlify-cms-proxy-server": "^1.3.24",
    "node-minify": "^3.6.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss": "^8.4.14",
    "prettier": "2.7.1",
    "serve": "^13.0.2",
    "tailwindcss": "^3.1.3",
    "ts-node": "^10.8.1",
    "ts-prune": "^0.10.3",
    "typescript": "^4.7.3",
    "yaml-lint": "^1.7.0"
  },
  "engines": {
    "node": ">=16.13.x",
    "npm": "USE_YARN"
  }
}
